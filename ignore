   "source": [
    "android_genres = freq_table(free_android, 1)\n",
    "\n",
    "for genre in android_genres:\n",
    "    total = 0\n",
    "    len_genre = 0\n",
    "    for rows in free_android:\n",
    "        genre_app = rows[1]\n",
    "        if genre_app == genre:\n",
    "            installs = rows[5]\n",
    "            installs = installs.replace(\"+\", \"\")\n",
    "            installs = installs.replace(\",\", \"\")\n",
    "            total += float(installs)\n",
    "            len_genre += 1\n",
    "    average = total / len_genre\n",
    "    print(genre, average)"
   ]
  },
